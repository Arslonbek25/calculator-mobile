{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:react-native/all"
	],
	"plugins": ["react", "react-native"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off",
		"react-native/no-raw-text": "off"
	}
}
